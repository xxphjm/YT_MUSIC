{
    "version": 2,
    "builds": [
      {
        "src": "run.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "run.py"
      }
  ],
  "env": {
    "PYTHONPATH": ".",
    "FFMPEG_BINARY": "/tmp/ffmpeg"
  },
  "functions": {
    "api/*.py": {
      "memory": 1024,
      "maxDuration": 300,
      "runtime": "edge"
    }
  }
}
